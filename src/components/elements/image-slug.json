{
  "collectionName": "components_elements_image_slugs",
  "info": {
    "displayName": "Image Slug",
    "description": ""
  },
  "options": {},
  "attributes": {
    "slug": {
      "type": "string",
      "required": true,
      "regex": "^[\\w\\s#-]+$"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "title": {
      "type": "string"
    },
    "bgColor": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    }
  }
}
